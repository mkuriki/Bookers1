<h1>Editing Book</h1>

<%= form_with model: @list, url: update_book_path(@list.id) do |f| %>
     <h4>Title</h4>
     <%= f.text_field :title %>
     <h4>Body</h4>
     <%= f.text_area :body %>
     
     <%= f.submit 'Update Book' %>
<% end %>

<%= link_to "show", book_path(@list) %>
<%= "|" %> 
<%= "destroy" %>